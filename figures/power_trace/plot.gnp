#!/usr/bin/gnuplot

reset

set lmargin 0
set rmargin 0
set tmargin -100
set bmargin -100

unset tics
unset key
unset label
unset border

set term png size 1500,500
set out 'figures/power_trace/power_trace.png'
set style line 1 lc rgb "black" lt 1 lw 2

plot 'figures/power_trace/power_trace.dat' w l ls 1